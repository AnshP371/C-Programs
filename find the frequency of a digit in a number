#include <stdio.h>

  int main()
  {
  int number,rem,count,digit=0;
  printf("enter the number:\n");
  scanf("%d",&number);
  printf("Enter the digit:\n");
  scanf("%d",&digit);
  while(number>0)
  {
  rem = number%10;
  number = number/10;
  if(rem==digit)
  {
  count++;
  }


  }
  printf("The frequency of digit %d in the number %d is : %d",digit,number,count);
  }
